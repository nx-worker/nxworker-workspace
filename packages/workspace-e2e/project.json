{
  "name": "workspace-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/workspace-e2e/src",
  "implicitDependencies": ["workspace"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/workspace-e2e/jest.config.ts",
        "runInBand": true
      },
      "dependsOn": ["^build"]
    },
    "benchmark": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/benchmarks/result.txt"],
      "options": {
        "command": "jest --projects packages/workspace-e2e/jest-bench.config.ts",
        "cwd": "{workspaceRoot}"
      }
    }
  }
}
